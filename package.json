{
  "name": "@abstracter/logman",
  "version": "0.0.0",
  "description": "Simple logging SDK",
  "private": false,
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abstracter-io/logman"
  },
  "scripts": {
    "outdated": "npx npm-check -u",
    "lint": "eslint --format pretty --ext=.js,.ts --fix",
    "lint:all": "npm run lint -- .",
    "lint:check": "eslint-config-prettier .eslintrc.js",
    "compile": "tsc --build tsconfig.json --pretty",
    "husky": "husky install",
    "test": "jest --runInBand --projects .",
    "build": "npm run lint && npm run compile",
    "clean": "rimraf dist",
    "clean:build": "npm run clean && npm run build",
    "commit": "git-cz"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "sdk",
    "logger",
    "simple"
  ],
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "serialize-error": "^11.0.0",
    "sonic-boom": "^3.3.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@abstracter/atomic-release": "^1.2.1-beta.2",
    "@abstracter/eslint-config": "^1.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^20.4.2",
    "benchmark": "^2.1.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-pretty": "^5.0.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "micromatch": "^4.0.5",
    "pino": "^8.14.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.1.6"
  }
}
